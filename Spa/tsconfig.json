{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "target": "es5",
        "lib": ["dom", "es6" ],
        "types": ["reflect-metadata", "jest", "node"],
        "module": "commonjs",
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "jsx": "react",
        "baseUrl": "./",
        "paths": {
          "uiBaseHook/*": ["src/UI/Base/Hooks/*"]
          "uiBaseContext/*": ["src/UI/Base/Context/*"]
          "uiBaseComponent/*": ["src/UI/Base/Components/*"]
          "ui/*": ["src/UI/*"]
          "requests/*": ["src/requests/*"]
          "domain/*": ["src/domain/*"]
        }
    }
}
