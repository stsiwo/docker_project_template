{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "target": "es5",
        "lib": ["dom", "es6" ],
        "types": ["reflect-metadata", "jest", "node"],
        "module": "commonjs",
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "jsx": "react",
        "baseUrl": "./",
        "paths": {
          "ui/*": ["src/UI/*"]
        }
    }
}
