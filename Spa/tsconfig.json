{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "target": "es5",
        "lib": ["dom", "es6" ],
        "types": ["reflect-metadata", "jest", "node"],
        "module": "commonjs",
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "esModuleInterop": true,
        "jsx": "react",
        "baseUrl": "./",
        "paths": {
          "Hooks/*": ["src/UI/Base/Hooks/*"],
          "Contexts/*": ["src/UI/Base/Context/*"],
          "Components/*": ["src/UI/Base/Components/*"],
          "ui/*": ["src/UI/*"],
          "requests/*": ["src/requests/*"],
          "domain/*": ["src/domain/*"],
          "configs/*": ["src/configs/*"],
          "actions/*": ["src/actions/*"],
          "states/*": ["src/states/*"],
          "reducers/*": ["src/reducers/*"],
          "src/*": ["src/*"]
        }
    }
}
